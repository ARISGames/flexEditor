<?xml version="1.0" ?>
<c:GameDataEditorView xmlns:mx="http://www.adobe.com/2006/mxml"
                      xmlns:c="org.arisgames.editor.components.*" creationPolicy="all">

    <mx:HBox id="editorPanel" width="100%" height="100%">
        <mx:Panel id="gameDisplayPanel" title="Game Information" height="100%">
            <mx:FormItem label="Name">
                <mx:Label id="gameName" width="200"/>
            </mx:FormItem>
            <mx:FormItem label="Description">
                <mx:Label id="gameDescription" width="200"/>
            </mx:FormItem>
        </mx:Panel>
    </mx:HBox>

</c:GameDataEditorView>