<?xml version="1.0"?>
<c:PlaceMarkerEditorMediaPickerView xmlns:mx="http://www.adobe.com/2006/mxml"
							 xmlns:main="org.arisgames.editor.*"
							 xmlns:v="org.arisgames.editor.view.*"
							 xmlns:c="org.arisgames.editor.components.*"
							 xmlns:controls="com.ninem.controls.*"
							 xmlns:containers="flexlib.mdi.containers"
							 paddingTop="10"
							 paddingLeft="10"
							 paddingRight="10"
							 paddingBottom="10"
							 >
	
	<controls:TreeBrowser id="treeBrowser" width="400" height="263" dataProvider="{xmlData}" showDetails="{selectButton.enabled}" horizontalScrollPolicy="auto" verticalScrollPolicy="off" showRoot="false"/>
	
	<mx:ControlBar id="folderControlBar">
		<mx:Spacer width="100%" id="spacer"/>
		<mx:Button id="closeButton" label="Close"/>
		<mx:Button id="selectButton" label="Select" enabled="false"/>
	</mx:ControlBar>
	
</c:PlaceMarkerEditorMediaPickerView>