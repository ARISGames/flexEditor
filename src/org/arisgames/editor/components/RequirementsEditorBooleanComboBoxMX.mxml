<?xml version="1.0"?>
<c:RequirementsEditorBooleanComboBoxView xmlns:mx="http://www.adobe.com/2006/mxml"
											 xmlns:c="org.arisgames.editor.components.*"
											 horizontalScrollPolicy="off"
											 verticalScrollPolicy="off"
											 >
	
	<mx:Binding destination="value" source="cbo.value"/>
	
	
	<mx:ComboBox id="cbo" editable="true" width="100%">
		<mx:dataProvider>
			<mx:String>AND</mx:String>
			<mx:String>OR</mx:String>
		</mx:dataProvider>
	</mx:ComboBox>
		
</c:RequirementsEditorBooleanComboBoxView>
